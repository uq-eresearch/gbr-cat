<module rename-to="cresis">
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User'/>
	<inherits name="com.google.gwt.maps.GoogleMaps" />
	<inherits name="com.gwtext.GwtExt" />
	<!-- inherits name="com.google.gwt.http.HTTP"/ -->
	<!-- inherits name='com.gwtextux.GwtExtUx'/ -->
	<!-- inherits name="com.gwtext.Charts" / -->
	<!-- inherits name="com.eg.gwt.openLayers.OpenLayers"/ -->

	<!-- Specify the app entry point class. -->
	<entry-point class='au.edu.uq.eresearch.client.Cresis'/>

	<!-- Specify the servlet. -->
	<servlet path="/server" class="au.edu.uq.eresearch.server.ServiceImpl"/>

	<!-- gbrcat.metadata.net key -->
  <script src="http://maps.google.com/maps?file=api&amp;v=2&amp;sensor=false&amp;key=ABQIAAAA7Y6FhZlMG6laxTCTHh7RrhSuTrH6Z2pVHS3veiumQZWew7D9sRTTOuDyjQtB1XJzTYzgLm1nd3huyA" />
